{
    "name": "misakstvanu/laravel-fortify-passkeys",
    "description": "Passkeys backend implementation for Laravel Fortify",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.28",
        "web-auth/webauthn-lib": "^4.7",
        "laravel/fortify": "^1.20"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Misakstvanu\\LaravelFortifyPasskeys\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Misakstvanu\\LaravelFortifyPasskeys\\PasskeysServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
